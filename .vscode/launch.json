{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Game",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/game.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
            ],
            "cwd": "${workspaceFolder}/webapp",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}